{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "lib/**"]
    },
    "test": {
      "outputs": ["test-results/**", "playwright-report/**"],
      "dependsOn": ["build"]
    },
    "dev": {
      "cache": false
    }
  }
}
